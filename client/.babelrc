{
	"plugins": [
		"syntax-dynamic-import",
		"transform-decorators-legacy",
		[
			"transform-runtime", {
				"polyfill": false,
				"regenerator": true
			}
		],
		["import", { libraryName: "antd", style: "css" }]
	],
	"presets": [
		"env",
		"react",
		"stage-0"
	],
	"env": {
		"start": {
			"presets": [
				"react-hmre"
			]
		}
	}
}

